<form method="post" action="{{ path('app_address_delete', {'id': address.id}) }}" onsubmit="return confirm('Confirmez-vous la supression ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ address.id) }}">
    <button class="btn btn-primary">
        <i class="fa fa-trash"></i>
        Supprimer
    </button>
</form>
